<launch>
<remap from="/move_iri_wam" to="/syn_move_iri_wam" />
<remap from="/iri_ros_controller/follow_joint_trajectory" to="/syn_iri_ros_controller/follow_joint_trajectory" />
    <node pkg="move_arm" type="move_arm_simple_action" name="iri_wam_move_arm" output="screen">
        <param name="group" type="string" value="iri_wam" />
        <remap from="arm_ik" to="iri_wam_iri_wam_kinematics/get_constraint_aware_ik" />
      
       <param name="move_arm_frequency" type="double" value="40" />
    </node>
    
    

</launch>
