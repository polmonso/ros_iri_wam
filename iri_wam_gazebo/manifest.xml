<package>
  <description brief="iri_wam_gazebo">
	Simulator of WAM arm robot in GAZEBO.
  </description>
  <author>IRI Robotics Lab, Ivan Rojas (irojas@iri.upc.edu)</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/iri_wam_gazebo</url>
  <depend package="gazebo"/>
  <depend package="gazebo_plugins"/>
  <depend package="roscpp"/>
  <depend package="arm_navigation_msgs" />
  <depend package="geometry_msgs" />
  <depend package="kinematics_msgs" />
  <depend package="trajectory_msgs"/>
  <depend package="control_msgs"/>
  <depend package="sensor_msgs"/>  
  <depend package="actionlib"/>
  <depend package="actionlib_msgs"/>
  <depend package="planning_environment"/>
  <depend package="iri_wam_common_msgs"/>
  <rosdep name="tinyxml" />
  <!-- <depend package="pr2_controllers_msgs"/>-->
  <export>
    <gazebo plugin_path="${prefix}/lib" />
  </export>  

</package>


