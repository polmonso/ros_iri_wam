<package>
  <description brief="iri_common_drivers_msgs">

     This stack contains all own-type messages for the different drivers

  </description>
  <author>Marti Morta, mmorta at iri.upc.ed</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/iri_common_drivers_msgs</url>
  <depend package="std_msgs"/>
  <depend package="actionlib"/>
  <depend package="actionlib_msgs"/>

  <export>
    <cpp cflags="-I${prefix}/msg/cpp"/>
  </export>

  <depend package="rosbag"/>
  <export>
    <rosbag migration_rule_file="migration_rules/iri_common_drivers_msgs.bmr"/>
  </export>

</package>


