<package>
  <description brief="iri_wam_controllers">

    This's package contain the PID configuration of real WAm to simulated WAM. This's pack use pr2_controllers & pr2_simulator packages
     
  </description>
  <author>IRI Robotics Lab, Ivan Rojas (irojas@iri.upc.edu)</author>
  <license>LGPL</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/iri_wam_controller</url>
  <depend package="roscpp"/>
  <depend package="actionlib"/>
  <depend package="arm_navigation_msgs"/>
  <depend package="actionlib_msgs"/>
  <depend package="control_msgs"/>
  <depend package="trajectory_msgs"/>
  <depend package="pr2_controllers_msgs"/>
  <depend package="pr2_controllers"/>
  <depend package="pr2_simulator"/>
    <export>
    <cpp cflags="-I${prefix}/msg/cpp -I${prefix}/include"/>
  </export>  
</package>


