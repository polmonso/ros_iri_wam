<launch>
  <!-- send table urdf to param server -->
  <param name="table_description" command="$(find xacro)/xacro.py $(find iri_wam_description)/xacro/cylinder_object.urdf.xacro" />

  <!-- push table_description to factory and spawn robot in gazebo -->
  <node name="spawn_table" pkg="gazebo" type="spawn_model" args="-urdf -param table_description -x 0.5 -y 0.14 -z 0.0 -model table_model" respawn="false" output="screen" />
</launch>
