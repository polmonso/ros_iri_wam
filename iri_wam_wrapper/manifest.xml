<package>
  <description brief="iri_wam_wrapper">
      Base package design to control Barrett WAM arm. <br />
      This driver is based on 
      <a href="http://wikiri.upc.es/index.php/WAM_Client_Server_Driver">
      IRI low level driver</a>.
  </description>
  <author>Pol Monso</author>
  <license>LGPL</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/iri_wam_wrapper</url>
  <depend package="roscpp"/>
  <depend package="iri_base_driver"/>
  <depend package="iri_action_server"/>
  <depend package="iri_wam_common_msgs"/> 
  <depend package="geometry_msgs"/>
  <depend package="sensor_msgs"/>
  <depend package="control_msgs"/>
  <depend package="eigen"/>
  <depend package="pr2_controllers_msgs"/>
  <depend package="trajectory_msgs"/>
</package>
