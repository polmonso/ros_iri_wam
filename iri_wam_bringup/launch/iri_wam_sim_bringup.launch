<launch> 

   <param name="/iri_wam_move_iri_wam/controller_action_name" type="string" value="iri_ros_controller/follow_joint_trajectory" />
    
<!-- Robot Description -->
    <include file="$(find iri_wam_description)/launch/iri_wam_description.launch" />

<!-- Gazebo Simulator -->
    <include file="$(find iri_wam_gazebo)/launch/iri_wam_gazebo.launch" />

<!-- Controller Manager -->
    <include file="$(find pr2_controller_manager)/controller_manager.launch" />

<!-- Add arm Controller -->
    <include file="$(find iri_wam_controllers)/launch/iri_ros_controller.launch" />

<!-- start arm navigation stack -->
   <include file="$(find iri_wam_arm_navigation)/launch/iri_wam_arm_navigation.launch" />
   
</launch>
